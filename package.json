{
  "name": "astro-nanointl",
  "type": "module",
  "version": "0.0.2",
  "description": "Tiny set of tools to implement internationalization for Astro",
  "author": {
    "name": "e3stpavel"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/e3stpavel/astro-nanointl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e3stpavel/astro-nanointl.git"
  },
  "bugs": {
    "url": "https://github.com/e3stpavel/astro-nanointl/issues"
  },
  "keywords": [
    "astro",
    "withastro",
    "astro-component",
    "astro-integration"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "middleware": [
        "dist/middleware/index.d.ts"
      ],
      "client": [
        "client.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "tsup --format esm --dts",
    "lint": "tsc && eslint .",
    "vitest": "vitest run",
    "release": "pnpm build && changeset publish"
  },
  "peerDependencies": {
    "astro": "^2.0.0-beta.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.39.5",
    "@changesets/cli": "^2.26.2",
    "@nanostores/i18n": "^0.10.0",
    "astro": "^2.6.4",
    "eslint": "^8.43.0",
    "tsup": "^7.0.0",
    "vite-plugin-virtual": "^0.2.0",
    "vitest": "^0.32.2"
  }
}
